import java.util.Scanner;
public class Pattern1ForDemo2{
     public static void main(String[] args)
{
  int n;
  Scanner sc= new Scanner(System.in);
  System.out.println("Enter the no of rows to be needed");
    
  n=sc.nextInt();
  System.out.println("\n");
  
  for(int i=1;i<=n;i++)
  {
  for(int p=1;p<=n;p++)
  {
     if(p<=i)
	 {
	 System.out.print(p);
	 }
	 else
     System.out.print(i);	 
     }
  System.out.println("\n");	
}	 
}
}
